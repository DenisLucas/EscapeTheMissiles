script/source = "extends KinematicBody2D

signal hit
var speed = 200
var velocidade = Vector2()

func _physics_process(delta: float) -> void:
	velocidade = get_input()
	velocidade = velociade_calc(velocidade, speed)
	velocidade = velocidade * delta
	velocidade = move_and_collide(velocidade)

func get_input() -> Vector2:
	var velociade = Vector2()
	if Input.is_action_pressed(\"ui_left\"):
		velociade.x -= 1
	if Input.is_action_pressed(\"ui_right\"):
		velociade.x += 1
	if Input.is_action_pressed(\"ui_up\"):
		velociade.y -= 1
	if Input.is_action_pressed(\"ui_down\"):
		velociade.y += 1
	velocidade = velociade.normalized() * get_process_delta_time()
	return velociade

func velociade_calc(velocidade, speed) -> Vector2:
	var out = velocidade
	if out.x != 0:
		out.x = out.x * speed
	if out.y != 0:
		out.y = out.y * speed
	print(out)
	return out


func _on_dead_body_entered(body: Node) -> void:
	if body.name == \"misseis\":
		emit_signal(\"hit\")
	elif body.name == \"misseis2\":
		emit_signal(\"hit\")
	elif body.name == \"misseis3\":
		emit_signal(\"hit\")
	elif body.name == \"misseis4\":
		emit_signal(\"hit\")	
"

